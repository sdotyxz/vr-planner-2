; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="VR_Planner_2"
run/main_scene="res://src/levels/MainLevel.tscn"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

AudioManager="*res://src/autoload/AudioManager.gd"
VFXManager="*res://src/autoload/VFXManager.gd"
GameManager="*res://src/autoload/GameManager.gd"
InputHandler="*res://src/autoload/InputHandler.gd"
LeaderboardManager="*res://src/autoload/LeaderboardManager.gd"

[display]

window/size/viewport_width=576
window/size/viewport_height=324
window/size/mode=3
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/SceneBuilder/plugin.cfg")

[input]

fire={
"deadzone": 0.5,
"events": [Object(InputEventMouseButton,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"button_mask":1,"position":Vector2(0, 0),"global_position":Vector2(0, 0),"factor":1.0,"button_index":1,"canceled":false,"pressed":true,"double_click":false,"script":null)
]
}

[shader_globals]

diffuse_curve={
"type": "sampler2D",
"value": "res://Shaders/diffuse_curve.tres"
}
specular_smoothness={
"type": "float",
"value": 0.05
}
fresnel_smoothness={
"type": "float",
"value": 0.05
}
outline_width={
"type": "float",
"value": 4.0
}
outline_color={
"type": "color",
"value": Color(0, 0, 0, 1)
}
